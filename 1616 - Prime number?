#include <bits/stdc++.h>
using namespace std;

bool f(int num){
    if (num == 1) return false;
    for (int i = 2; i * i <= num; ++i){
        if (num  % i == 0) return false;
    }
    return true;
}



int main(){
    int num;
    cin >> num;
    if (f(num))
        cout << "Yes";
    else cout << "No";
    
    return 0;
}
